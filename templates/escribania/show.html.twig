{% extends 'base.html.twig' %}

{% block title %}Escribania{% endblock %}

{% block body %}
    <h1>Escribania</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ escribanium.id }}</td>
            </tr>
            <tr>
                <th>Direccion</th>
                <td>{{ escribanium.direccion }}</td>
            </tr>
            <tr>
                <th>Telefono</th>
                <td>{{ escribanium.telefono }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ escribanium.email }}</td>
            </tr>
            <tr>
                <th>Foto</th>
                <td>{{ escribanium.foto }}</td>
            </tr>
            <tr>
                <th>Latitud</th>
                <td>{{ escribanium.latitud }}</td>
            </tr>
            <tr>
                <th>Longitud</th>
                <td>{{ escribanium.longitud }}</td>
            </tr>
            <tr>
                <th>Estado</th>
                <td>{{ escribanium.estado ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ escribanium.nombre }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('escribania_index') }}">back to list</a>

    <a href="{{ path('escribania_edit', {'id': escribanium.id}) }}">edit</a>

    {{ include('escribania/_delete_form.html.twig') }}
